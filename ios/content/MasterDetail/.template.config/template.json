{
  "author": "Microsoft",
  "classifications": [
    "ios",
    "app",
    "general"
  ],
  "name": "Master Detail App",
  "defaultName": "MasterDetailApp1",
  "description": "Creates an iOS application with a Storyboard that contains a UISplitViewController that navigates to a detail view when a row is selected in the master view.",
  "groupIdentity": "Xamarin.iOS.MasterDetailApp",
  "identity": "Xamarin.iOS.MasterDetailApp.CSharp",
  "shortName": "ios-masterdetailapp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "NewApp",
  "preferNameDirectory": true,
  "symbols": {
    "AppIdentifier": {
      "type": "parameter",
      "description": "Overrides the Info.plist's CFBundleIdentifier",
      "replaces": "com.xamarin.masterdetailapp",
      "datatype": "string",
      "defaultValue": "com.companyname"
    },
    "MinimumOSVersion": {
      "type": "parameter",
      "description": "Overrides the Info.plist's MinimumOSVersion",
      "replaces": "$MinimumOSVersion",
      "datatype": "string",
      "defaultValue": "8.0"
    },
    "UseUniversal": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    },
    "UseIPhone": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    },
    "UseIPad": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    }
  },
  "sources": [
    {
      "source": "../base/"
    },
    {
      "source": "./",
      "target": "NewApp/"
    }
  ]
}