{
  "author": "Microsoft",
  "classifications": [
    "iOS",
    "mobile",
    "Xamarin"
  ],
  "name": "Xamarin.iOS App",
  "groupIdentity": "Xamarin.iOS.App",
  "defaultName": "App",
  "description": "Creates a simple, one-page iOS application with a Storyboard that contains the View, and a View Controller class to manage it.\n\nUse this as a starting point for your first iOS application.",
  "identity": "Xamarin.iOS.App.CSharp",
  "shortName": "ios-app",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "NewApp",
  "primaryOutputs": [
    {
        "path": "NewApp.csproj"
    },
    {
      "condition": "CreateUITestProject",
      "path": "../NewApp.UITests/NewApp.UITests.csproj"
    }
  ],
  "guids": [
    "80A96B52-6758-4BDB-B0B4-CEA4CA155C60",
    "3BC97AEA-CE2A-4B7A-A1B0-7E49B61539EC"
  ],
  "preferNameDirectory": true,
  "symbols": {
    "kind": {
      "type": "parameter",
      "datatype": "choice",
      "defaultValue": "blank",
      "choices": [
        {
          "choice": "blank",
          "description": "An empty Xamarin.iOS app"
        },
        {
          "choice": "single-view",
          "description": "A Xamarin.iOS app with a single view"
        },
        {
          "choice": "tabbed",
          "description": "A Xamarin.iOS app with tabbed navigation"
        },
        {
          "choice": "master-detail",
          "description": "A Xamarin.iOS app with master-detail navigation"
        },
        {
          "choice": "web-view",
          "description": "A Xamarin.iOS app with a web view"
        },
        {
          "choice": "page-based",
          "description": "A Xamarin.iOS app with a page view"
        }
      ]
    },
    "DeviceFamily": {
      "type": "parameter",
      "datatype": "choice",
      "defaultValue": "universal",
      "choices": [
        {
          "choice": "universal",
          "description": "Support both iPhone and iPad devices"
        },
        {
          "choice": "iphone",
          "description": "Support only iPhone devices"
        },
        {
          "choice": "ipad",
          "description": "Support only iPad devices"
        }
      ]
    },
    "AppIdentifier": {
      "type": "parameter",
      "description": "Overrides the Info.plist's CFBundleIdentifier",
      "replaces": "com.companyname.NewApp",
      "datatype": "string",
      "defaultValue": "com.companyname.NewApp"
    },
    "MinimumOSVersion": {
      "type": "parameter",
      "description": "Overrides the Info.plist's MinimumOSVersion",
      "replaces": "MinOSVersion",
      "datatype": "string",
      "defaultValue": "11.2"
    },
    "IncludeXamarinEssentials": {
        "type": "parameter",
        "dataType": "bool",
        "defaultValue": "false"
    },
    "XamarinEssentialsSdk": {
      "type": "parameter",
      "dataType": "string",
      "replaces": "XamarinEssentialsSdk",
      "defaultValue": "1.0.0"
    },
    "CreateUITestProject": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    }
  },
  "sources": [
    {
      "source": "./NewApp"
    },
    {
      "source": "../blank/",
      "condition": "(kind == \"blank\")"
    },
    {
      "source": "../single-view/",
      "condition": "(kind == \"single-view\")"
    },
    {
      "source": "../master-detail/",
      "condition": "(kind == \"master-detail\")"
    },
    {
      "source": "../tabbed/",
      "condition": "(kind == \"tabbed\")"
    },
    {
      "source": "../page-based/",
      "condition": "(kind == \"page-based\")"
    },
    {
      "source": "../web-view/",
      "condition": "(kind == \"web-view\")"
    },
    {
      "source": "../uitest",
      "target": "../NewApp.UITests/",
      "condition": "CreateUITestProject"
    }
  ]
}