{
  "author": "Microsoft",
  "classifications": [
    "ios",
    "app",
    "general"
  ],
  "name": "iOS Blank App",
  "defaultName": "BlankApp",
  "description": "Creates a simple, one-page iOS application with a Storyboard that contains the View, and a View Controller class to manage it.\n\nUse this as a starting point for your first iOS application.",
  "groupIdentity": "Xamarin.iOS.BlankApp",
  "identity": "Xamarin.iOS.BlankApp.CSharp",
  "shortName": "ios-blankapp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "NewApp",
  "preferNameDirectory": true,
  "symbols": {
    "AppIdentifier": {
      "type": "parameter",
      "description": "Overrides the Info.plist's CFBundleIdentifier",
      "replaces": "com.xamarin.singleview",
      "datatype": "string",
      "defaultValue": "com.companyname"
    },
    "MinimumOSVersion": {
      "type": "parameter",
      "description": "Overrides the Info.plist's MinimumOSVersion",
      "replaces": "$MinimumOSVersion",
      "datatype": "string",
      "defaultValue": "8.0"
    },
    "UseUniversal": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    },
    "UseIPhone": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    },
    "UseIPad": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    }
  },
  "sources": [
    {
      "source": "../base/",
      "exclude": [
        "Info.plist"
      ]
    },
    {
      "source": "./",
      "target": "NewApp/"
    }
  ]
}