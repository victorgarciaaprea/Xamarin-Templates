{
  "author": "Microsoft",
  "classifications": [
    "ios",
    "app",
    "general"
  ],
  "name": "Tabbed App",
  "defaultName": "TabbedApp1",
  "description": "Creates an iOS application with a Storyboard that contains a UITabBarController with bottom tabbed navigation for two child views.",
  "groupIdentity": "Xamarin.iOS.TabbedApp",
  "identity": "Xamarin.iOS.TabbedApp.CSharp",
  "shortName": "ios-tabbedapp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "TabbedApp",
  "preferNameDirectory": true,
  "symbols": {
    "AppIdentifier": {
      "type": "parameter",
      "description": "Overrides the Info.plist's CFBundleIdentifier",
      "replaces": "com.xamarin.tabbedapp",
      "datatype": "string",
      "defaultValue": "com.companyname"
    },
    "MinimumOSVersion": {
      "type": "parameter",
      "description": "Overrides the Info.plist's MinimumOSVersion",
      "replaces": "$MinimumOSVersion",
      "datatype": "string",
      "defaultValue": "8.0"
    },
    "UseUniversal": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    },
    "UseIPhone": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    },
    "UseIPad": {
      "type": "parameter",
      "dataType": "bool",
      "defaultValue": "false"
    },
  },
  "primaryOutputs": [
    {
      "path": "TabbedApp.csproj"
    }
  ],
}