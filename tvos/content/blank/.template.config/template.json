{
  "author": "Microsoft",
  "classifications": [
    "tvos",
    "apple",
    "Xamarin"
  ],
  "name": "Xamarin.tvOS App",
  "groupIdentity": "Xamarin.tvOS.Template",
  "defaultName": "App",
  "description": "Creates a basic tvOS application with a Storyboard containing a single view, and a View Controller class to manage it.\n\nUse this as a starting point for your first tvOS application.",
  "identity": "Xamarin.tvOS.App.CSharp",
  "shortName": "tvos-app",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "NewApp",
  "preferNameDirectory": true,
  "symbols": {
    "AppIdentifier": {
      "type": "parameter",
      "description": "Overrides the Info.plist's CFBundleIdentifier",
      "replaces": "com.companyname.NewApp",
      "datatype": "string",
      "defaultValue": "com.companyname.NewApp"
    },
    "MinimumOSVersion": {
      "type": "parameter",
      "description": "Overrides the Info.plist's MinimumOSVersion",
      "replaces": "MinOSVersion",
      "datatype": "string",
      "defaultValue": "11.2"
    }
  }
}